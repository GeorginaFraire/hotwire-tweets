<%= turbo_frame_tag dom_id(tweet) do %>
  <div class="card card-body">
    <div><%= tweet.body %></div>

    <div class="mt-2">
    <%= button_to "Likes(#{tweet.likes_counter})", tweet_likes_path(tweet) , method: :post %>
      <%# Likes (0) %>
      <%# Retweets (0) %>
    </div>
  </div>
<% end %>